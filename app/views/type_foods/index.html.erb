<p id="notice"><%= notice %></p>

<h1>Type Foods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_foods.each do |type_food| %>
      <tr>
        <td><%= type_food.name %></td>
        <td><%= link_to 'Show', type_food %></td>
        <td><%= link_to 'Edit', edit_type_food_path(type_food) %></td>
        <td><%= link_to 'Destroy', type_food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Food', new_type_food_path %>
